\end{Soutput}
\end{Schunk}

\begin{Schunk}
\begin{Sinput}
> plot(res, only.sig = TRUE, adj.method = "BH", cutoff = 0.1)
\end{Sinput}
\end{Schunk}
\begin{figure}[htbp]
